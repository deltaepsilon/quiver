{
  "name": "@quiver/storage-uploader",
  "version": "0.0.1",
  "license": "MIT",
  "main": "./src/index.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run -s serve || npm run -s dev",
    "build": "preact build --no-prerender --template template.html",
    "serve": "preact build --no-prerender && preact serve",
    "dev": "preact watch --template template.html --cwd .",
    "lint": "eslint src",
    "test": "jest --watch"
  },
  "eslintConfig": {
    "extends": "eslint-config-synacor"
  },
  "eslintIgnore": [
    "build/*"
  ],
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-jest": "^21.2.0",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "chai": "^4.1.2",
    "eslint": "^4.5.0",
    "eslint-config-synacor": "^1.1.0",
    "html-looks-like": "^1.0.3",
    "identity-obj-proxy": "^3.0.0",
    "if-env": "^1.0.0",
    "jest": "^21.2.1",
    "node-sass": "^4.7.2",
    "preact-cli": "^2.0.0",
    "preact-compat-enzyme": "^0.2.5",
    "preact-jsx-chai": "^2.2.1",
    "preact-test-utils": "^0.1.3",
    "sass-loader": "^6.0.6",
    "webpack": "3.7.0"
  },
  "dependencies": {
    "linkstate": "^1.1.0",
    "preact": "^8.2.1",
    "preact-compat": "^3.17.0",
    "preact-material-components": "^1.2.4",
    "rxjs": "^5.5.5"
  },
  "jest": {
    "setupFiles": [
      "./jest-setup.js"
    ],
    "testURL": "http://localhost:8080",
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less|scss)$": "identity-obj-proxy",
      "^react$": "preact-compat",
      "^react-dom$": "preact-compat"
    },
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(@material|preact-material-components))"
    ]
  }
}
